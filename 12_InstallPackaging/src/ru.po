#: guess.c:32
#, c-format
msgid "Usage: ./guess [OPTIONS]\n"
msgstr "Использование: ./guess [Опции]\n"

#: guess.c:33
#, c-format
msgid "    OPTIONS:\n"
msgstr "    Опции:\n"

#: guess.c:34
#, c-format
msgid "        --help  print help message\n"
msgstr "        --help  вывести вспомогательное сообщение\n"

#: guess.c:35
#, c-format
msgid "        -r      use Roman numbers\n"
msgstr "        -r      использование римских цифр\n"

#: guess.c:42
#, c-format
msgid "Guess a number from 1 to 100.\n"
msgstr "Загадайте число от 1 до 100.\n"

#: guess.c:49
#, c-format
msgid "Is your number greater than %s? (yes/no)\n"
msgstr "Число больше %s?\n"

#: guess.c:51
#, c-format
msgid "Is your number greater than %zu? (yes/no)\n"
msgstr "Число больше %zu?\n"

#: guess.c:58
msgid "yes"
msgstr "да"

#: guess.c:60
msgid "no"
msgstr "нет"

#: guess.c:65
#, c-format
msgid "Your number is %s\n"
msgstr "Ваше число %s\n"

#: guess.c:67
#, c-format
msgid "Your number is %zu\n"
msgstr "Ваше число: %zu\n"